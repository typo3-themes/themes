{
  "name": "typo3-themes/themes",
  "type": "typo3-cms-extension",
  "description": "TYPO3 THEMES",
  "homepage": "http://typo3-themes.org",
  "license": ["GPL-2.0+"],
  "keywords": ["TYPO3 CMS", "TYPO3 THEMES", "THEMES", "THEMES", "FLUID", "CUSTOMIZE"],
  "require": {
    "typo3/cms-core": "^12.4",
    "typo3/cms-form": "^12.4",
    "gridelementsteam/gridelements": ">=12.0.0 || dev-master || dev-ea_12-0 || dev-bugfix-fix-gridelements-plugin",
    "ext-json": "*",
    "ext-pdo": "*"
  },
  "require-dev": {
    "typo3/testing-framework": "^6.16.6"
  },
  "support": {
    "issues": "https://github.com/typo3-themes/themes/issues"
  },
  "autoload": {
    "psr-4": { "KayStrobach\\Themes\\": "Classes/"}
  },
  "autoload-dev": {
    "psr-4": { "KayStrobach\\Themes\\Tests\\": "Tests/"}
  },
  "replace": {
    "typo3-ter/themes": "self.version"
  },
  "conflict": {
    "typo3-ter/yaml-parser": "*"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "themes"
    }
  }
}
